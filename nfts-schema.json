{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "array",
    "items": {
      "type": "object",
      "required": ["name", "contracts"],
      "properties": {
        "name": {
          "type": "string",
          "minLength": 1
        },
        "contracts": {
          "type": "array",
          "items": {
            "type": "string",
            "pattern": "^0x[a-fA-F0-9]{40}$"
          }
        }
      },
      "additionalProperties": false
    }
  }